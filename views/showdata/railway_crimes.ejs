<% include ../../partials/header.ejs %>


<div class="container center">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">case_no</th>
                <th scope="col">train_no</th>
                <th scope="col">stolen_stuff</th>
                <th scope="col">train_status</th>
                <th scope="col">victim_details</th>
            </tr>
        </thead>
        <tbody>

            <ul>
                <% for(var i=0; i<queryOutput.length; i++) { %>
                <tr>
                    <td>
                        <%= queryOutput[i].case_no %>
                    </td>
                    <td>
                        <%= queryOutput[i].train_no %>
                    </td>
                    <td>
                        <%= queryOutput[i].stolen_stuff %>
                    </td>
                    <td>
                        <%= queryOutput[i].train_status %>
                    </td>
                    <td>
                        <%= queryOutput[i].victim_details %>
                    </td>
                </tr>
                <% } %>
            </ul>
        </tbody>
    </table>
</div>

<% include ../../partials/footer.ejs %>